---
exclude: !ruby/regexp '/
  \.(tmp|swp)$
  |
  \.(?:autotest|gemtest|gitignore|hoerc|minitest.rb|simplecov-prelude.rb)
  |
  CVS\/
  |
  ^(?i:TAGS)
  |
  \.(?:
      DS_Store|
      bundle|
      git|
      hg|
      idea|
      svn|
      vagrant
    )\/
  |
  Appraisals
  |
  [gG]emfile(?:\.lock)?
  |
  \.(coveralls|pullreview|travis|rubocop.*)\.yml$
  |
  \.byebug_history
  |
  .bundle\/
  |
  \.gemspec
  |
  Vagrantfile
  |
  test\/generators/rails-\w+/app/models
  |
  test\/generators/rails-\w+/db/migrate
  |
  test\/generators/rails-\w+/db/schema\.rb
  |
  test\/generators/rails-\w+/db/\w+\.sqlite3
  |
  test\/generators/rails-\w+/log/\w+\.log
  /x'
